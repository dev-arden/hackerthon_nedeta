<div class="container" style="margin-top:50px">

    <form action="/complain/write" method="POST">
      <div class="form-group">
        <label for="exampleInputEmail1">제목</label>
        <input type="text" name="title" class="form-control" id="exampleInputEmail1" placeholder="신고 제목을 알려주세요">
      </div>
      
      <div class="form-group">
          <label for="comment">내용: </label>
          <textarea class="form-control" name="content" rows="5" id="comment" placeholder="신고 내용을 알려주세요"></textarea>
      </div>
          
      <button type="submit" class="btn btn-default">Submit</button>
    </form>

</div>